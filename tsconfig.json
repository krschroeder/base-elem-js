{
    "include": ["src/js/**/*.ts"],
    "exclude": ["src/js/tests.ts"],
    "compilerOptions": {
        "module": "esnext",
        "moduleResolution": "node",
        "target": "esnext",
        "jsx": "preserve",
    
        "resolveJsonModule": true,
        "sourceMap": false,
        "esModuleInterop": true,
        "verbatimModuleSyntax": true,
        "skipLibCheck": true,
        "strictNullChecks": false,
      }
}